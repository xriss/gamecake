    if test "x$CC" = "xgcc"; then
        CCVERSION=`$CC --version | grep 4.[2-9].`
        if test "x$CCVERSION" != "x"; then
            echo "$as_me:$LINENO: WARNING: gcc >= 4.2 cannot compile 10.4 compatible code. Using gcc 4.0 instead."
            CC="gcc-4.0"
        fi
    fi

