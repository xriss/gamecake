cd `dirname $0`

echo " remember to use --clean to force a full rebuild "

echo build windows

../vbox_mingw/make $*


echo build emscripten

../vbox_emcc/make $*


echo build 64bit ubuntu 16.04 version

../vbox_1604_64/make $*


echo build 32bit ubuntu 16.04 version 

../vbox_1604_32/make $*


echo build 64bit OSX 10.14 version

../vbox_osx/make $*


echo build arm/raspi

../vbox_arm/make $*


echo show status, this should list new versions of everything we just built

cd ../exe

git status

echo add and commit changed files
./git-publish
